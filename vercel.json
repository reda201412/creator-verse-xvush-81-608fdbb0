{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/api/mux/direct-upload",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/mux/direct-upload.ts"
    },
    {
      "src": "/api/oss/upload-thumbnail",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/oss/upload-thumbnail.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "status": 404,
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "MUX_TOKEN_ID": "614f11e3-39f6-46ef-8104-680fda482a19",
